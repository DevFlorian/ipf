<xds:queryRegistry xmlns:xds="http://www.openehealth.org/ipf/xds">
    <xds:getAllQuery>
        <type value="GetAllQuery" type="OFFICIAL"/>
        <homeCommunityId>12.21.41</homeCommunityId>
        <extraParameters/>

        <!-- =============== VALUES MARKED AS OFFICIAL =============== -->

        <!-- 0. Official value, marked as official - shall be accepted as is -->
        <documentStatus value="Approved" type="OFFICIAL"/>

        <!-- 1. Non-official valid value, marked as official - shall be recognized as user-defined -->
        <documentStatus value="UserDefined" type="OFFICIAL"/>

        <!-- 2. Non-official invalid value, marked as official - shall be recognized as invalid -->
        <documentStatus value="urn:oasis:names:tc:ebxml-regrep:StatusType:SuperDuper" type="OFFICIAL"/>

        <!-- =============== VALUES MARKED AS USER-DEFINED =============== -->

        <!-- 3. Official value, marked as user-defined - shall be accepted as is -->
        <documentStatus value="Approved" type="USER_DEFINED"/>

        <!-- 4. Non-official valid value, marked as user-defined - shall be accepted as is -->
        <documentStatus value="UserDefined" type="USER_DEFINED"/>

        <!-- 5. Non-official invalid value, marked as user-defined - shall be recognized as invalid -->
        <documentStatus value="urn:oasis:names:tc:ebxml-regrep:StatusType:SuperDuper" type="USER_DEFINED"/>

        <!-- =============== VALUES MARKED AS INVALID =============== -->

        <!-- 6. Official value, marked as invalid - shall be accepted as is -->
        <documentStatus value="Approved" type="INVALID"/>

        <!-- 7. Non-official valid value, marked as invalid - shall be accepted as is -->
        <documentStatus value="UserDefined" type="INVALID"/>

        <!-- 8. Non-official invalid value, marked as invalid - shall be accepted as is -->
        <documentStatus value="urn:oasis:names:tc:ebxml-regrep:StatusType:SuperDuper" type="INVALID"/>

    </xds:getAllQuery>
</xds:queryRegistry>
